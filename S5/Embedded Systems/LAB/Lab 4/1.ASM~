	LDA 4500H
	ANI 01H
	JZ EVEN
ODD:	LXI H, 2000H
        LXI D, 4502H
        MVI B, 0AH
NEXTO: 	MOV A, M
        STAX D
        INX H
        INX D
        DCR B
        JNZ NEXTO
	JMP END
EVEN:	LXI H, 2000H
        LXI D, 4502H
        MVI B, 0AH
NEXT:  	MOV A, M
        STAX D
        INX H
        INX D
        DCR B
        JNZ NEXT
END:	HLT